import java.util.Scanner;

public class University {
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		
		public Student getStudent(String name; String lastName; int index) {
			name=input.nextLine();
			lastName=input.nextLine();
			index=input.nextInt();
			input.nextLine();
			return Student(name, lastName, index)
			
			
		}
		
	}

}
